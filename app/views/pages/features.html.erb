<div class="text-center container-fluid bg-gray ">
  <% if current_user.present? %>
    <% active_subscription = current_user.active_subscription %>
    <% subscription_type  = active_subscription.nil? ? nil : active_subscription.paid? ? nil : active_subscription.subscription_type_text  %>
    <% subscription_name =  active_subscription.present? ? active_subscription.name : 'Basic' %>
    <% renewal = active_subscription.nil? ? 'N/A' : active_subscription.lifetime? ? 'N/A' : short_date(active_subscription.ends_on) %>
  <% else %>
    <% subscription_type = nil %>
    <% subscription_name  = 'N/A' %>
    <% renewal = 'N/A' %>
  <% end %>
  <div class="row-fluid">

    <div class="col-md-6">
      <h3 class="text-primary wow fadeInUp"  data-wow-delay="0.1s"  >My Subscription</h3>
      <h3 class="mt0 wow fadeInUp" data-wow-delay="0.5s"><strong><%= subscription_type %> <%= subscription_name %></strong></h3>
    </div>
    <div class="col-md-6">
      <h3 class="text-primary wow fadeInUp" data-wow-delay="0.1s">My Renewal Date</h3>
      <h3 class="mt0 wow fadeInUp" data-wow-delay="0.5s"> <strong><%=  renewal %></strong></h3>
    </div>
  </div>

  <div class="row-fluid">
    <% if active_subscription.present? && active_subscription.next_plan.present?%>
      <div class="col-md-12">
        <p> Note: Your <%= active_subscription.subscription_plan.name  %>  Subscription Will Change To <%= active_subscription.next_plan.titleize %> On Your Upcoming Renewal Date
      </div>
    <% end %>

    <div class="col-md-4 col-md-offset-4">
      <div class="form-group">
        <% if current_user.nil? %>
          <%= link_to 'Modify Subscription', new_user_session_path, class: 'btn btn-lightgrey btn-full'%>
        <% elsif current_user.active_subscription.present? %>
            <%= link_to 'Modify Subscription', profile_subscription_path, remote: true ,class: 'btn btn-lightgrey btn-full'%>
        <% else %>
           <%= link_to 'Subscribe', new_stripe_subscription_path, remote: true,
                    class: 'btn btn-lightgrey btn-full', data: {disable_with: "Subscribe <i class='fa fa-spinner fa-spin'></i>"} %>
        <% end %>
      </div>
    </div>
  </div>
</div>
<div class="clearfix"></div>
