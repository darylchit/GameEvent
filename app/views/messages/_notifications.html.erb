<div class="col-md-9">
  <h3 class="mt0"><%= @box_type %></h3>
  <% if @box.count > 0 %>
    <ul class=" list-unstyled list-group messages">
      <% @box.each do | notification | %>
        <%= render partial: "notification", locals: {notification: notification} %>
      <% end %>
    </ul>
    <div id='clan_application_notification'></div>
  <% else %>
    No Notifications
  <% end %>
</div>
