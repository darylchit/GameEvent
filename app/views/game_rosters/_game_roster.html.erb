                  <span class="fa fa-bullseye"></span></button>
              </div>
            </div>
          </div>
          <% end%>
          <% if players.present? %>
            <div class="row">
              <div class="row-fluid event-players-headers">Players</div>
            </div>
            <% players.each do | invite | %>
              <div class='<%= "remove_player#{invite.id}"%>'>
                <div class="row player-row">
                  <div class="col-xs-3 user-avatar">
                    <%= link_to image_tag(invite.user.avatar_url, class: 'user_avatar_img'), player_path(invite.user) %>
                  </div>
                  <div class="col-xs-9 player-data">
                    <div class="row">
                      <div class="col-xs-12 set_font_size">
                        <b font-size="14px"><%= invite.user.username %></b>
                      </div>
                    </div>
