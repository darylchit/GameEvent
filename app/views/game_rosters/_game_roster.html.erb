                </div>
                <div class="col-xs-6 set_font_size"><span class="text-primary">Exp: </span>
                  <%= event.user.event_experience %>
                </div>
              </div>
              <div class="row">
                <div class="col-xs-6 set_font_size"><span class="text-primary">Age: </span><%= event.user.age.present? ? event.user.age : ""%></div>
                <div class="col-xs-6 set_font_size">
                  <span class="text-primary">CR:</span>
                  <%= event.user.event_cancellation_rate %>%
                </div>
              </div>
              <div class="row">
                <div class="col-xs-6 set_font_size"><span class="text-primary">Time Zone: </span><%= event.user.timezone_abbr %></div>
