                        <span class="text-primary">Patience: </span>
                        <%= invite.user.newbie_patience_level %>
                      </div>
                    </div>
                    <div class="set_plus_position">
                      <button class="btn-link" data-toggle="modal" data-target="#game_roster_actions_invite<%= invite.id %>">
                        <span class="fa fa-bullseye"></span></button>
                    </div>
                  </div>
                </div>
                <%= render partial: "game_rosters/game_roster_actions", locals: { invite: invite } %>
              </div>
            <% end %>
          <% end %>
          <!-- <div class="row gold"></div> -->
          <% if waitlisted.present? %>
            <div class="row">
              <div class="row-fluid event-players-headers">Waitlist</div>
            </div>
