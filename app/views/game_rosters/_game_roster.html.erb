                            <i class="fa fa-thumbs-up thump-pos" aria-hidden="true"></i>
                          </span>
                        </span>
                      </div>
                      <div class="col-xs-6 set_font_size">
                        <span class="text-primary">Exp: </span>
                        <%= invite.user.event_experience %>
                      </div>
                    </div>
                    <div class="row">
                      <div class="col-xs-6 set_font_size">
                        <span class="text-primary">Age: </span>
                        <%= invite.user.age.present? ? invite.user.age : ""%>
                      </div>
                      <div class="col-xs-6 set_font_size">
                        <span class="text-primary">CR: </span>
                        <%= invite.user.event_cancellation_rate %>%
                      </div>
                    </div>
                    <div class="row">
                      <div class="col-xs-6 set_font_size"><span class="text-primary">Time Zone: </span><%= invite.user.timezone_abbr %></div>
