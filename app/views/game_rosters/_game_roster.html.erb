<div class="background-block game_roster_page">
  <div class="contaner-fluid lg hidden-xs">
    <% if false%>
    <div class="row">
      <div class="col-md-12 col-sm-12">
        <div class="col-md-6 col-sm-6">
          <h3 class="mt0 event-title hide">Event Type:   </h3>
        </div>
        <div class="col-md-6 col-sm-6 text-right-md text-right-sm">
          <% if !event.cancelled? && !event.completed? %>
            <% link_string , link_path, method = get_link_for_invite(event)%>
            <%= link_to link_string, link_path, method: method, remote: (current_user.present? ? true : false), id: "event_action#{event.id}", class: "btn btn-primary event_btn_size cancelled_event_link event_action#{event.id}"%>
          <% end %>
          <% if (current_user.present? && !event.cancelled? && !event.completed?)  %>
              <% if (event.user_id == current_user.id) || (event.clan_event? && event.clan.host_id == current_user.id) %>
                  <button type="button" class="btn btn-primary cancelled_event_link event_btn_size share_btn" data-toggle="modal" data-target="#cancel_event_confirmation">
                    Cancel
                  </button>
              <% end %>

          <% end %>
          <% if !event.cancelled? %>
            <%= link_to 'Share', share_game_roster_path(event), remote: true, class: 'btn btn-primary cancelled_event_link event_btn_size share_btn' %>
          <% end %>
        </div>
       </div>
    </div>
    <% end %>
    <div class="row">
      <div class="player-card">
        <div class="event-image">
          <%= image_tag(event.game.game_jumbo, :class=> '') %>
          <div class="event-spots hide">
            <span>Spots  Filled: <%= event.get_spot %></span>
          </div>
        </div>
        <div class="row-fluid game_roster_trasperent gold-row">
          <div class="col-sm-2 col-xs-3 padding_right_0 padding_left_0">
            <%= link_to image_tag(event.user.avatar_url, class: 'set_user_img'), player_path(event.user) %>
          </div>
          <div class="col-sm-10 col-xs-9 padding_left_0 padding_right_0 roster_side_data">
            <div class="set_box_design">
              <h4 class="text-primary set_font_size set_mrg_top mrg_top_10 title_wrap">
                <span class="text-primary event-title"><%= event.event_type.present? ? event.event_type.titleize : ""%> | <%= event.game.title  %> (<%= event.game_system.abbreviation.upcase   %>)</span>
              </h4>
              <div class="row">
                <div class="col-md-4 col-sm-4">
                  <div> <span class="text-primary">Starts:</span><span class=""> <%= event.start_at.strftime("%b-%d | %I:%M%p") %></span></div>
                  <div> <span class="text-primary">Duration:</span><span class=""> <%= event.duration/60 %> Hours</span></div>
                </div>
                <div class="col-md-4 col-sm-4">
                  <div>
                    <span class="text-primary">Age Limits: </span>
                    <span class=""><%= event.minimum_age.present? ?  event.minimum_age.to_s : "13" %>
                      <%= " - " + ( event.maximum_age.present? ? event.maximum_age.to_s : "99" ) %></span>
                  </div>
                  <div>
                    <span class="text-primary">Play Style: </span>
                    <span class=""><%= event.play_type.present? ? event.play_type.titleize : "" %></span>
                  </div>
                </div>
                <div class="col-md-4 col-sm-4">
                  <div>
                    <span class="text-primary">Spots  Filled: </span>
                    <span><%= event.get_spot %></span>
                  </div>
                  <div>
                    <span class="text-primary">Status: </span>
                    <span class=""><%= event.status.present? ? event.status.split('_').map(&:titleize).join(' - ') : ""%></span>
                  </div>
                </div>
              </div>
              <h6 class="set_font_size set_event_title title_wrap">
                <a data-toggle="modal" data-target="#event_title_popup" class="cursor_pointer"><span class="text-white"><%= event.title.present? ? event.title.upcase : "" %></span></a>
              </h6>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
  <div class="container">
    <div class="lg hidden-xs">
      <div class="row set_top_mrg">
        <% if event.details.present? %>
          <div class="col-sm-12">
            <p class="text-gray text-justify"><%= event.details %></p>
          </div>
        <% end %>
      </div>
      <%if current_subscription.nil? %>
        <div class="col-md-12 text-center form-group">
          <script async src="//pagead2.googlesyndication.com/pagead/js/adsbygoogle.js"></script>
          <!-- Desktop Long Banner -->
          <ins class="adsbygoogle"
               style="display:inline-block;width:970px;height:90px"
               data-ad-client="ca-pub-7969073226896485"
               data-ad-slot="7194419117"></ins>
          <script>
          (adsbygoogle = window.adsbygoogle || []).push({});
          </script>
        </div>
      <% else %>
        <div class="form-group"></div>
      <% end %>

      <% if true %>
        <div class="col-md-12 form-group">
          <ul class="nav nav-tabs">
            <li class="active right hide ">
              <a data-toggle="tab" href="#game_roster_data" class="btn-lightgrey">Game Roster</a>
            </li>
            <li class="right hide">
              <a data-toggle="tab" href="#roster_chat" class="btn-lightgrey">Roster Chat</a>
            </li>
            <% if !event.cancelled?  %>
              <% if !event.completed? %>
                <li class="right">
                  <% link_string , link_path, method = get_link_for_invite(event)%>
                  <%= link_to link_string, link_path, method: method, remote: (current_user.present? ? true : false), id: "event_action#{event.id}", class: "btn btn-lightgrey"%>
                </li>
              <% else %>
                <li class="right">
                  <a class="btn btn-lightgrey btn-full" onClick="CustomeToastr('This Command Is Not Valid For Completed Events')">
                    Edit Event
                  </a>
                </li>
              <% end %>
            <% end %>
            <% if (current_user.present? && !event.cancelled? && !event.completed?)  %>
              <% if (event.user_id == current_user.id) || (event.clan_event? && event.clan.host_id == current_user.id) %>
                  <li class="right">
                    <a class="btn btn-lightgrey btn-full" data-toggle="modal" data-target="#cancel_event_confirmation">
                      Cancel
                    </a>
                  </li>
              <% else %>
                <li class="right">
                  <a class="btn btn-lightgrey btn-full" onClick="CustomeToastr('Only The Host Can Cancel This Roster')">
                    Cancel
                  </a>
                </li>
              <% end %>
            <% elsif !event.cancelled?  %>
              <li class="right">
                <a class="btn btn-lightgrey btn-full" onClick="CustomeToastr('This Command Is Not Valid For Completed Events')">
                  Cancel
                </a>
              </li>
            <% end %>
            <% if !event.cancelled? %>
              <li class="right">
                <%= link_to 'Share', share_game_roster_path(event), remote: true, class: 'btn btn-lightgrey' %>
              </li>
            <% end %>
          </ul>

        </div>
        <!-- <div class="col-md-12">
          <hr class="gold_line row topmrg_20 ">
        </div> -->
      <% end %>
    </div>

    <div class="xs visible-xs">
      <div class="row">
        <div class="player-card">
          <% if false %>
            <button class="btn-link add-icon-top plus-btn">
              <span class="fa fa-plus"></span>
            </button>
            <div class="player-add-menu">
              <ul class="list-unstyled">
                <% if !event.cancelled? && !event.completed? %>
                  <%link_string , link_path, method = get_link_for_invite(event)%>
                  <li>
                    <%= link_to link_string, link_path, method: method, remote: (current_user.present? ? true : false), id: "event_action#{event.id}", class: "btn btn-primary event_btn_size cancelled_event_link event_action#{event.id}" %>
                  </li>
                <% end %>
                <% if current_user.present? and !event.cancelled? && !event.completed? %>
                    <% if (event.user_id == current_user.id)  || (event.clan_event? && event.clan.host_id == current_user.id) %>
                        <li>
                          <button type="button" class="btn btn-primary cancelled_event_link event_btn_size share_btn" data-toggle="modal" data-target="#cancel_event_confirmation">
                            Cancel
                          </button>
                        </li>
                    <% end %>
                <% end %>
                <li>
                  <%= link_to 'Share', share_game_roster_path(event), remote: true, class: 'btn btn-primary cancelled_event_link event_btn_size' %>
                </li>
              </ul>
            </div>
          <% end %>
          <div class="event-image">
            <%= image_tag(event.game.game_jumbo_mobile, :class=> '') %>
          </div>
          <div class="gold-header-row game_roster_trasperent">
            <div class="col-xs-3 padding_left_0 padding_right_0">
              <%= link_to image_tag(event.user.avatar_url, class: 'set_user_img'), player_path(event.user) %>
            </div>
            <div class="col-xs-9 padding_left_0 padding_right_0 roster_side_data">
              <div class="event-data padding_left_10">
                <h4 class="text-primary  set_game_name title_wrap">
                  <%= event.game.title.present? ? event.game.title : ""%>
                </h4>
                <div class="set_font_size">
                  <%= event.event_type.present? ? event.event_type.titleize : ""%> <%=(event.event_type.present? && event.status.present?) ? " | " : ""%><span class="event_status"><%= event.status.present? ? event.status.split('_').map(&:titleize).join(' - ') : ""%>
                </div>

                <div class="set_font_size">
                  Starts: <%= event.start_at.present? ? event.start_at.strftime("%b-%d | %I:%M%p" ) : 'N/A' %>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
      <div class="row event-information set_top_mrg">
        <div class="col-xs-6">
          <div class="span_data set_font_size">
            <span class="text-primary">Age Limits: </span>
            <span class="text-gray"><%= event.minimum_age.present? ?  event.minimum_age.to_s : "13" %>
              <%= " - " + ( event.maximum_age.present? ? event.maximum_age.to_s : "99" ) %></span>
          </div>
        </div>

        <div class="col-xs-6">
          <div class="span_data set_font_size">
            <span class="text-primary">System: </span>
            <span class="text-gray"><%= event.game_system.abbreviation.present? ? event.game_system.abbreviation.upcase : "" %></span>
          </div>
        </div>

        <div class="col-xs-6">
          <div class="span_data set_font_size">
            <span class="text-primary">Play Style: </span>
            <span class="text-gray"><%= event.play_type.present? ? event.play_type.titleize : "" %></span>
          </div>
        </div>

        <div class="col-xs-6">
          <div class="span_data set_font_size">
            <span class="text-primary">Host Patience: </span>
            <span class="text-gray">
              <% if event.recurring_event? %>
                N/A
              <% else %>
                <%= event.user.newbie_patience_level %>
              <% end %>
            </span>
          </div>
        </div>

        <div class="col-xs-6">
          <div class="span_data set_font_size">
            <span class="text-primary">Duration: </span>
            <span class="text-gray"><%= event.duration/60 %> Hour</span>
          </div>
        </div>

        <div class="col-xs-6">
          <div class="span_data set_font_size">
            <span class="text-primary">Spots Filled: </span>
            <span class="text-gray"><%= event.get_spot%></span>
          </div>
        </div>

        <div class="col-xs-12 mrg_top_10">
          <div class="span_data set_font_size title_wrap">
            <a data-toggle="modal" data-target="#event_title_popup" class="cursor_pointer"><span class="text-white"><%= event.title.present? ? event.title.upcase : "" %></span></a>
          </div>
        </div>

        <div class="col-xs-12">
          <div class="span_data set_font_size ">
            <p class="text-gray text-justify"><%= event.details %></p>
          </div>
        </div>
      </div>

      <% if current_subscription.nil? %>
      <hr class="gold_line mrg_0"/>
        <div class="text-center row" style="margin-top: 10px">
          <script async src="//pagead2.googlesyndication.com/pagead/js/adsbygoogle.js"></script>
          <!-- Mobile Banner -->
          <ins class="adsbygoogle"
               style="display:inline-block;width:320px;height:100px"
               data-ad-client="ca-pub-7969073226896485"
               data-ad-slot="4553256987"></ins>
          <script>
          (adsbygoogle = window.adsbygoogle || []).push({});
          </script>
        </div>
      <% else %>
        <hr class="gold_line mt0"/>
      <% end %>
      <div class="form-group">


        <% if !event.cancelled? && !event.completed? %>
          <%link_string , link_path, method = get_link_for_invite(event)%>
            <%= link_to link_string, link_path, method: method, remote: (current_user.present? ? true : false), id: "event_action#{event.id}", class: "btn btn-lightgrey btn-full cancelled_event_link event_action#{event.id}" %>
        <% end %>
        <% if current_user.present? and !event.cancelled? && !event.completed? %>
            <% if (event.user_id == current_user.id)  || (event.clan_event? && event.clan.host_id == current_user.id) %>
              <button type="button" class="btn btn-lightgrey btn-full cancelled_event_link" data-toggle="modal" data-target="#cancel_event_confirmation">
                Cancel
              </button>
            <% end %>
        <% end %>
        <% if !event.cancelled? %>
         <%= link_to 'Share', share_game_roster_path(event), remote: true, class: 'btn btn-lightgrey btn-full cancelled_event_link' %>
        <% end %>


      </div>
      <% if !event.recurring_event? || players.present? || waitlisted.present? || pending_invitations.present? || no_show.present? %>
      <div class="mobile_tabs hide">
        <ul class="nav nav-tabs">
          <!-- <li class="active width_50 txt_center"> -->
          <div class="col-xs-6 " style="padding-left: 0px">
            <a data-toggle="tab" href="#game_roster_data_mobile" class="btn-lightgrey active btn btn-full">Game Roster</a>
          </div>
          <div class="col-xs-6" style="padding-right: 0px">
          <!-- </li> -->
          <!-- <li class="width_50 txt_center"> -->
            <a data-toggle="tab" href="#roster_chat" class="btn-lightgrey btn btn-full">Roster Chat</a>
          </div>
          <!-- </li> -->
        </ul>
      </div>
      <!-- <hr class="gold_line mrg_btm_10"/> -->
      <% elsif event.recurring_event? &&  %>
        <h2 class="text-center"> No Participants On This Roster </h2>
      <% end %>
    </div>

    <div class="tab-content1">
      <div id="game_roster_data" class="">
          <!-- Desktop Table -->
          <div class="row-fluid set_top_mrg hidden-xs">
            <table class="table game_data">
              <thead></thead>

              <% if event.recurring_event? %>
                <% unless players.present? || waitlisted.present? || pending_invitations.present? || no_show.present? %>
                <tr>
                   <td class="padding_bottom_0 padding_top_0"> <h5 class="event-players-headers" colspan="9">Participants</h5></td>
                </tr>
                <tr class="player-row">
                  <td class="set_game_img_width set_relative">
                    <%= image_tag('avatars/dreamstimeextrasmall_13680895.jpg', class: 'avatar_img') %>
                  </td>
                  <td class="set_align width_25">
                    <h3>Be The First To Join This Event</h3>
                  </td>
                </tr>

                <% end %>
              <% else %>
                <tr>
                   <td class="padding_bottom_0 padding_top_0"> <h5 class="event-players-headers" colspan="9">Host</h5></td>
                </tr>
                <%= render partial: 'public_games/players', locals: {player: event.user, invite: event.invites.first}  %>
              <% end %>

              <% if players.present? %>
                <tr>
                  <td colspan="9" class="padding_bottom_0" > <h5 class="event-players-headers">Players</h5></td>
                </tr>

                <% players.each do | invite | %>
                  <%= render partial: 'public_games/players', locals: {player: invite.user, invite: invite}  %>
                  <% unless invite.equal? players.last %>
                    <tr>
                      <td class="gray_line padding_top_0 padding_bottom_0" colspan="7"></td>
                    </tr>
                  <% end %>
                <% end %>
              <% end %>

              <!-- <tr>
                <td class="padding_0" colspan="9"><hr class="gold_line"/></td>
              </tr> -->

              <% if waitlisted.present? %>
                <tr>
                  <td class="padding_bottom_0" > <h5 class="event-players-headers">Waitlist</h5></td>
                </tr>
                <% waitlisted.each do | invite | %>
                  <%= render partial: 'public_games/players', locals: {player: invite.user, invite: invite}  %>
                  <% unless invite.equal? waitlisted.last %>
                    <tr>
                      <td class="gray_line padding_top_0 padding_bottom_0" colspan="7"></td>
                    </tr>
                  <% end %>
                <% end %>
              <% end %>

              <% if pending_invitations.present? %>
                <tr>
                  <td class="padding_bottom_0" colspan="9"> <h5 class="event-players-headers">Pending Invitations</h5></td>
                </tr>
                <% pending_invitations.each do | invite | %>
                  <%= render partial: 'public_games/players', locals: {player: invite.user, invite: invite}  %>
                  <% unless invite.equal? pending_invitations.last %>
                    <tr>
                      <td class="gray_line padding_top_0 padding_bottom_0" colspan="7"></td>
                    </tr>
                  <% end %>
                <% end %>

              <% end %>

              <% if no_show.present? %>
                <tr>
                  <td class="padding_bottom_0" colspan="9"><h5 class="event-players-headers">No Show</h5></td>
                </tr>
                <% no_show.each do | invite | %>
                  <%= render partial: 'public_games/players', locals: {player: invite.user, invite: invite}  %>
                  <% unless invite.equal? no_show.last %>
                    <tr>
                      <td class="gray_line padding_top_0 padding_bottom_0" colspan="7"></td>
                    </tr>
                  <% end %>
                <% end %>
              <% end %>

              <% if false #waitlisted.present?  || pending_invitations.present? || no_show.present? %>
                <tr>
                  <td class="padding_0" colspan="9"><hr class="gold_line"/></td>
                </tr>
              <% end %>

              <tr class="hide">
                <td colspan="9"><span class="text-primary">Event Number: </span><span><%#= event.token %></span></td>
              </tr>
            </table>
          </div>
      </div>
      <div id="game_roster_data_mobile" class="tab-pane fade in active">
        <div class="visible-xs col-xs-12">
          <% if event.recurring_event? %>
          <% else %>
            <div class="row">
              <div class="row-fluid event-players-headers">Host</div>
            </div>
            <div class="row player-row">
            <div class="col-xs-3 user-avatar">
              <%= link_to image_tag(event.user.avatar_url, class: 'user_avatar_img'), player_path(event.user) %>
            </div>
            <div class="col-xs-9  player-data">
              <div class="row">
                <div class="col-xs-12 set_font_size">
