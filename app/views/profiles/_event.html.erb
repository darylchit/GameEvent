<tr>
  <td class="">
    <%= link_to event.game.title.titleize, get_event_show_path(event), class: 'text-white cursor_pointer' %>
  </td>
  <td class="text-center">
    <%= link_to event.event_type.titleize, get_event_show_path(event), class: 'text-white cursor_pointer' %>
  </td>
  <td class="text-center">
    <%= link_to event.status.titleize, get_event_show_path(event), class: 'text-white cursor_pointer' %>
  </td>
  <td class="text-center" data-toggle="tooltip" data-placement="top" data-title="<%= event.start_at.strftime("%b %e %Y | %I:%M %p") %>">
    <%= link_to event.start_at.strftime("%b %-d | %l:%M%P"), get_event_show_path(event), class: 'text-white cursor_pointer' %>
  </td>
</tr>
